	<!-- Light Animations for Green App lights -->
    <animation>
        <type>noshadow</type>
        <object-name>GreenLight10000</object-name>
    </animation>

    <animation>
        <type>alpha-test</type>
		<object-name>GreenLight10000</object-name>
        <alpha-factor>0.01</alpha-factor>
    </animation>
	<!--
	<animation>
		<type>select</type>
		<object-name>strobe10000</object-name>
			<condition>
				<equals>
				<property>/scenery/LOWI/LOWI_r26_app_flash/light-number</property>
				<value>1.0</value>
				</equals>
			</condition>
    </animation>
	-->
	
	<animation>
		<type>select</type>
		<object-name>GreenLight10000</object-name>
			<condition>
				<or>
					<greater-than>
						<property>/sim/time/sun-angle-rad</property>
						<value>1.4742</value>
					</greater-than>
					<less-than>
